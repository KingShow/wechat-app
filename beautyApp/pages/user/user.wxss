/**order.wxss**/
.containerwrap{
  position: fixed;
  top: 0;
  right: 0;
  width: 100%;
  height: 100%;
  background-color: #f5f5f5;
}
.usercontainer{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 66rpx 0;
  box-sizing: border-box;
}
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.listitem{
  height: 88rpx;
  line-height: 88rpx;
  padding-left: 30rpx;
  border-bottom: 1px solid #f5f5f5;
  font-size: 28rpx;
  color: #444;
  background-color: #fff;
}